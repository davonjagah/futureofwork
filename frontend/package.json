{
	"name": "deela",
	"version": "0.1.0",
	"private": true,
	"homepage": "/",
	"dependencies": {
		"@apollo/client": "^3.3.4",
		"@hookform/resolvers": "^2.6.0",
		"@types/node": "18.11.18",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.10",
		"axios": "^0.20.0",
		"cross-env": "^7.0.3",
		"express": "^4.17.1",
		"git-branch-is": "^4.0.0",
		"globby": "^11.0.1",
		"graphql": "^15.4.0",
		"next": "13.1.2",
		"node-fetch": "^2.6.1",
		"open": "^8.4.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-bootstrap": "^1.6.1",
		"react-hook-form": "^7.10.1",
		"react-scripts": "^4.0.1",
		"react-visibility-sensor": "^5.1.1",
		"recoil": "0.0.13",
		"styled-components": "^5.2.1",
		"styled-system": "^5.1.5",
		"styled-theming": "^2.2.0",
		"swiper": "^6.8.1",
		"typescript": "4.9.4",
		"wait-on": "^7.0.1",
		"yup": "^0.32.9",
		"@project-serum/anchor": "^0.26.0",
		"@solana/web3.js": "^1.73.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"assert": "^2.0.0"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "cross-env NODE_ENV=production next start -p $PORT",
		"gen": "graphql-codegen --config codegen.yml",
		"storybook": "start-storybook -p 6006 -s ./public",
		"build-storybook": "build-storybook -s ./public",
		"cypress:open": "cypress open",
		"format": "prettier --write \"./**/*.{js,ts,json}\"",
		"lint": "eslint . --ext ts --ext tsx --ext js --fix",
		"lint:styled": "stylelint './src/**/*.styled.ts'",
		"type-check": "tsc --pretty --noEmit",
		"test": "jest --passWithNoTests",
		"fix-styles": "prettier-stylelint --write 'src/**/*.{styled.ts}'",
		"test-all": "npm run lint && npm run lint:styled && npm run type-check && npm run test",
		"sitemap": "node scripts/generate-sitemap",
		"release": "node script/deployment/autoTag.js",
		"prepare": "husky install",
		"pre-commit": "lint-staged",
		"test:pass": "jest --passWithNoTests"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/preset-env": "^7.12.7",
		"@graphql-codegen/cli": "1.19.4",
		"@graphql-codegen/typescript": "1.19.0",
		"@graphql-codegen/typescript-operations": "1.17.12",
		"@graphql-codegen/typescript-react-apollo": "^2.2.1",
		"@storybook/addon-actions": "^6.1.14",
		"@storybook/addon-essentials": "^6.1.14",
		"@storybook/addon-links": "^6.1.14",
		"@storybook/node-logger": "^6.1.14",
		"@storybook/preset-create-react-app": "^3.1.5",
		"@storybook/react": "^6.1.14",
		"@testing-library/dom": "^7.28.1",
		"@testing-library/jest-dom": "^5.11.6",
		"@testing-library/react": "^11.2.2",
		"@testing-library/user-event": "^12.5.0",
		"@types/jest": "^26.0.15",
		"@types/node": "^12.0.0",
		"@types/styled-components": "^5.1.4",
		"@types/styled-system": "^5.1.10",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"babel-jest": "^26.6.3",
		"babel-loader": "8.1.0",
		"babel-plugin-react-require": "^3.1.3",
		"concurrently": "^7.4.0",
		"cypress": "^6.0.1",
		"eslint": "^7.15.0",
		"eslint-config-next": "^13.0.5",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-cypress": "^2.11.2",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"husky": "^6.0.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^26.6.3",
		"jest-styled-components": "^7.0.3",
		"jest-watch-typeahead": "^0.6.1",
		"lint-staged": "^11.0.0",
		"nodemon": "^2.0.6",
		"prettier": "^2.2.1",
		"prettier-stylelint": "^0.4.2",
		"request": "^2.88.2",
		"stylelint": "^13.13.1",
		"stylelint-a11y": "^1.2.3",
		"stylelint-config-recommended": "^3.0.0",
		"stylelint-config-styled-components": "^0.1.1",
		"stylelint-processor-styled-components": "^1.10.0",
		"ts-node": "^9.1.1",
		"webpack": "4.42.0"
	},
	"husky": {
		"hooks": {
			"type-check": "yarn type-check",
			"pre-commit": "npx lint-staged"
		}
	},
	"lint-staged": {
		"./**/*.{js,jsx,ts,tsx}": [
			"prettier --write",
			"npm run lint",
			"npm run lint:styled",
			"git add"
		],
		"/src/**/*.scss": [
			"prettier --write",
			"stylelint --fix",
			"git add"
		],
		"{*.{json,md}}": [
			"prettier --write",
			"git add"
		]
	},
	"engines": {
		"npm": ">=8.0.0 <9.0.0",
		"node": ">=16.0.0 <17.0.0"
	}
}
